{
  "name": "uploader",
  "version": "1.1.4",
  "exports": "./main.ts",
  "tasks": {
    "dev": "deno run --allow-read --allow-write --allow-net --allow-run --allow-env main.ts",
    "check": "deno check main.ts",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "test": "deno test --allow-read --allow-write --allow-net --allow-env --allow-run",
    "build": "deno compile --allow-read --allow-write --allow-net --allow-run --allow-env --output bin/uploader main.ts"
  },
  "imports": {
    "@std/yaml": "jsr:@std/yaml@^1",
    "@std/path": "jsr:@std/path@^1",
    "@std/fs": "jsr:@std/fs@^1",
    "@std/fmt": "jsr:@std/fmt@^1",
    "@std/cli": "jsr:@std/cli@^1",
    "@std/assert": "jsr:@std/assert@^1",
    "@std/testing": "jsr:@std/testing@^1",
    "@std/testing/bdd": "jsr:@std/testing@^1/bdd",
    "ssh2": "npm:ssh2@^1"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true
  }
}
